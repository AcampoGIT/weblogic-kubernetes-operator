apiVersion: v1
kind: Service
metadata:
  name: internal-weblogic-operator-svc
  namespace: %OPERATOR_NAMESPACE%
  labels:
    weblogic.resourceVersion: operator-v1
    weblogic.operatorName: %OPERATOR_NAMESPACE%
spec:
  type: ClusterIP
  selector:
    app: weblogic-operator
  ports:
    - port: 8082
      name: rest